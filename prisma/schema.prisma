generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model characters {
  ID            Int    @id @default(autoincrement()) @db.UnsignedInt
  CharacterName String @db.VarChar(50)
}
